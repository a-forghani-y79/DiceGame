<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #005ecb;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.mainMenuController">
   <children>
      <JFXButton layoutX="124.0" layoutY="14.0" mnemonicParsing="false" opacity="0.0" prefWidth="1.0" text="Button" />
      <JFXButton fx:id="btnSingle" onAction="#onClickSinglePlayer" buttonType="RAISED" layoutX="50.0" layoutY="80.0" prefWidth="200.0" style="-fx-background-color: #83b9ff;" text="SinglePlayer" textAlignment="CENTER" textFill="#c41025">
         <font>
            <Font name="Mj_Calligraph" size="25.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnSettings" buttonType="RAISED" layoutX="80.0" layoutY="240.0" prefWidth="140.0" style="-fx-background-color: #83b9ff;" text="Settings" textAlignment="CENTER" textFill="#c41025">
         <font>
            <Font name="Mj_Calligraph" size="25.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnMulty" onAction="#onClickMultiPlayer" buttonType="RAISED" layoutX="50.0" layoutY="160.0" prefWidth="200.0" style="-fx-background-color: #83b9ff;" text="MultiPlayer" textAlignment="CENTER" textFill="#c41025">
         <font>
            <Font name="Mj_Calligraph" size="25.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnExit" buttonType="RAISED" layoutX="110.0" layoutY="320.0" onAction="#onClickExit" prefWidth="80.0" style="-fx-background-color: #83b9ff;" text="Exit" textAlignment="CENTER" textFill="#c41025">
         <font>
            <Font name="Mj_Calligraph" size="25.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
